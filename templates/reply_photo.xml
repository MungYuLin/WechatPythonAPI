$def with (toUser, fromUser, createTime, news, count, title, description, picurl, url)
<xml>
    <ToUserName>
        <![CDATA[$toUser]]>
    </ToUserName>
    <FromUserName>
        <![CDATA[$fromUser]]>
    </FromUserName>
    <CreateTime>$createTime</CreateTime>
    <MsgType>
        <![CDATA[$news]]>
    </MsgType>
    <ArticleCount>$count</ArticleCount>
    <Articles>
        <item>
            <Title>
                <![CDATA[$title]]>
            </Title>
            <Description>
                <![CDATA[$description]]>
            </Description>
            <PicUrl>
                <![CDATA[$picurl]]>
            </PicUrl>
            <Url>
                <![CDATA[$url]]>
            </Url>
        </item>
    </Articles>
</xml>
